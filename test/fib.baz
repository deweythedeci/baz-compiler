# this is a basic implementation of a fibonacci sequence generator

void main();
    print("Hello, world!\n");
    fibonacci(10);
end;


int fibonacci(int n);
    if(n < 1);
        return 0;
    else if(n == 1);
        return 1;
    else;
        return fibonacci(n-1) + fibonacci(n-2);
    end;
end;